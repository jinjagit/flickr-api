<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:find_by, url: photos_path, method: :get) do |f| %>

      <%= f.label :id %>
      <%= f.text_field :id, required: true, class: 'form-control field', style: "width:80%;margin-left:10%;", placeholder: "134813803@N03" %>

      <%= f.submit "find by ID", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

<br><br>
<%= link_to "find photos", photos_path, style: "color: blue" %>
